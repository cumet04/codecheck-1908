<article>
  <section class="project_info">
    <%= image_tag(thumb_project_path(@project), :class => "thumb") %>
    <div class="title"> <%= @project.title %> </div>
    <div class="description"> <%= @project.description %> </div>
    <div class="url">
      <a href=<%= @project.url %> > <%= @project.url %> </a>
    </div>
  </section>
</article>
